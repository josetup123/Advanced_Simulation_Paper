@inproceedings{li_new_2013,
	title = {A {New} {Multiple} {Traveling} {Salesman} {Problem} and {Its} {Genetic} {Algorithm}-{Based} {Solution}},
	doi = {10.1109/SMC.2013.112},
	abstract = {This work formulates for the first time a multiple traveling salesman problem (MTSP) with ordinary and exclusive cities, denoted by MTSP for short. In the original MTSP, a city can be visited by any traveling salesman and is thus renamed as an ordinary one in MTSP. A new class of cities is introduced in MTSP, called exclusive ones. They are divided into groups, each of which can be exclusively visited by a specified or predetermined salesman. To solve MTSP, a genetic algorithm is presented. It encodes cities and salesman into two single chromosomes. Accordingly, three modes of crossover and mutation operators are designed, i.e., simple city crossover and mutation (CCM), simple salesman crossover and mutation, and mixed city-salesman crossover and mutation. All the operations of crossover and mutation follow the proper relationship between cities and salesman. With the help of an MTSP example, the performance of the proposed algorithm with three modes of crossover and mutation operators is compared and analyzed. The simulation results show that the algorithm can solve MTSP with rapid convergence with CCM being the best mode of the operators.},
	booktitle = {2013 {IEEE} {International} {Conference} on {Systems}, {Man}, and {Cybernetics}},
	author = {Li, Jun and Sun, Qirui and Zhou, MengChu and Dai, Xianzhong},
	month = oct,
	year = {2013},
	note = {ISSN: 1062-922X},
	keywords = {Biological cells, Cities and towns, Convergence, Educational institutions, Encoding, Genetic Algorithm, Genetic algorithms, Multiple Traveling Salesman Problem, optimization, Traveling salesman problems},
	pages = {627--632},
	file = {IEEE Xplore Abstract Record:/Users/user/Zotero/storage/4F7IJW2U/6721865.html:text/html;IEEE Xplore Full Text PDF:/Users/user/Zotero/storage/QZJ7RC6B/Li et al. - 2013 - A New Multiple Traveling Salesman Problem and Its .pdf:application/pdf},
}

