	keywords = {aerial photography, crime scene/accident investigations, forensics, real-time evidence detection, UAV drones},
	pages = {1221--1232},
	file = {Full Text PDF:/Users/user/Zotero/storage/P4RF3A47/Georgiou et al. - 2022 - UAV-assisted real-time evidence detection in outdo.pdf:application/pdf;Snapshot:/Users/user/Zotero/storage/P2SNWHDN/1556-4029.html:text/html},
